{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES6",
    "lib": [ "es5", "dom", "es2015.promise", "scripthost" ],
    "outFile": "js/maintainable_game.js"
  },
  "include": [
    "Libs/Utils/**/*",
    "Libs/Sponsor/**/*",
    "lib/**/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
